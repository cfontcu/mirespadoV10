1. CONSULTA DEL ARTICULO,FEJECUTAFORM("VDARTIC","S","CODART=:CODART")
2. CONSULTA DE LA RECEPCION,FEJECUTAFORM("VDMONITORRECEP","S","CODRECEP=:CODRECEP")
3. MOVIMIENTOS DE LA RECEPCION,FEJECUTAFORM("VDMOVIMASC","S","CODMOV IN (SELECT CODMOV FROM VDMOVIM WHERE CODART=:CODART AND CODLOT=:CODLOT AND CODRECEP = :CODRECEP AND TAREA = VD.GETPROP('TAREARECEP') AND STATUS < #STMOVANULADO AND CODUBIORI='FUENTEREC')")
4. MOVIMIENTOS DEL ARTICULO-LOTE DE LA RECEPCION,FEJECUTAFORM("VDMOVIMASC","S","CODMOV IN (SELECT CODMOV FROM VDMOVIM WHERE CODART=:CODART AND CODLOT=:CODLOT AND CODRECEP = :CODRECEP AND STATUS < VDST.FMOVANULADO)")
5. STOCK DEL ARTICULO,FEJECUTAFORM("VDSTKALM","N","","CODART=:CODART","","CONSULTA DEL STOCK DEL ARTICULO :CODART","S")
6. CONSULTA DEL LOTE,FEJECUTAFORM("VDLOTES","S","CODART=:CODART AND CODLOT=:CODLOT")
7. STOCK DEL LOTE,FEJECUTAFORM("VDSTKLOT","N","CODART=:CODART AND CODLOT=:CODLOT","","","CONSULTA DEL STOCK DEL ARTICULO :CODART LOTE :CODLOT","S")
