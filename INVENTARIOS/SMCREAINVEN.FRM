CAMPO=NOINVENDESDE,OCULTO,"¿D-MM-Y.YY"
CAMPO=CICLICO,OCULTO,"_"
CAMPO=SOLOVACIAS,OCULTO,"_"
CAMPO=SWTUBISINMOV,OCULTO,"_"
CAMPO=MAXUBIAINVEN,OCULTO,"@L@@@@@"
CAMPO=PRIOINVEN,OCULTO,"@L"
