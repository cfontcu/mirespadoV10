CAMPO=NUMMARCADAS,AUXILIAR,OCULTO,"@L@@@@@"

PREFORM=FDESIGNACION("CPONDEFECTO","")

BEGINBLOQUE=SMCREAINVEN.PAN
  CURSOR=CPONDEFECTO SELECT 'S' INCHR,'S' INCRP,'S' INCPU,
                            TO_CHAR(TO_DATE(VD.GETPROP('INICIOANO')||'-'||TO_CHAR(SYSDATE,'YYYY'),'DD-MM-YYYY'),'J') NOINVENDESDE,
                            'S' CICLICO,100 MAXUBIAINVEN,70 PRIOINVEN,'N' SWTUBISINMOV,'N' SOLOVACIAS,
                            (SELECT  MIN(INVENANO)
                               FROM (SELECT (SELECT NVL(SUM(CASE WHEN FECFIN>=SMINVEN.DAMEINICIOANO(VD.FECHASYS) THEN 1 ELSE 0 END),0) INVENANO
                                       FROM VDINVENCONTEO
                                       WHERE CODUBI=UB.CODUBI AND STATUS=500) INVENANO
                                      FROM VDUBICA UB
                                      WHERE UB.TIPOUBI='R' AND VD.CONTIENE(VD.GETPROP('AREASNOINVEN'),UB.CODAREA)=0
                                      GROUP BY UB.CODUBI)) VECESCONTADO     
  FROM DUAL;
  TECLA=F2,PRESEHIJO
ENDBLOQUE


