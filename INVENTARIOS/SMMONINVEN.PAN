CAMPO=NUMINVASIGJF,AUXILIAR,CONTEXTUAL=FEJECUTAFORM("SMINVENTARIODESG","S","STATUS BETWEEN 150 AND 200 AND RECONTEO>=VD.GETPROP('CONTEOSJEFE')")
CAMPO=NUMINVTERJF
