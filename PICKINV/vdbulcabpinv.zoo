1.- CONSULTA DE LOS BULTOS QUE ABASTECE,FEJECUTAFORM("VDBULTOSMA","S","(CODBULTO) IN (SELECT DISTINCT BUL.CODBULTO FROM VDBULTOLIN BUL, VDBULTOLINPINV BPL WHERE BPL.CODBULTO=:CODBULTO AND BPL.CODMOV = BUL.CODMOV AND BPL.CODMOV <> 0)","")
2.- MOVIMIENTOS DEL BULTO,FEJECUTAFORM("VDMOVIM","S","(CODMATORI=:CODMAT OR CODMATDEST=:CODMAT)","","","MOVIMIENTOS DEL BULTO MATRICULA :CODMAT","S")
3.- CONSULTA DE LA MATRÍCULA DEL BULTO, FEJECUTAFORM("VDCONTE","S","CODMAT=:CODMAT","","","","S")
4.- CAMBIO DE RESERVA DE BULTO DE PICKING INVERSO,FEJECUTAFORM("VDPICKINVRES","S","CODBULTO=:CODBULTO AND STATUS BETWEEN VDST.FBPCPDTEETIQ AND VDST.FBPCFINALIZADO-1","","","","S")
5.- SOLICITAR SALIDA,FEJECUTA("CSOLSALIDA","CUBETA NO EN HDS")
6.- SALIDA A WMS,FEJECUTA("CSOLGARE10","CUBETA NO EN HDS")