BEGINBLOQUE=VDLISTAIMPRESORAS.PAN
  PREREGISTRO=PRESEHIJO
  AQUERY
  REGPAG=7
  POSX=1
  POSY=1
ENDBLOQUE    
BEGINBLOQUE=VDIMPRESPOOL.PAN
  PADRE=VDLISTAIMPRESORAS.PAN
  CAMPO=CODIMPREAUX,AUXILIAR,VIRTUAL("CODIMPRE"),OCULTO  
  PREQUERY=QUERYHIJO
  PREREGISTRO=PRESEHIJO
  WHERE=STATUS<1000 And CODIMPRE=:CODIMPREAUX;
  POSY=13
  POSX=1
  REGPAG=29
  ORDERBY=PRIORIDAD,SEQIMPRE;
  AQUERY
  TIMEOUT=QUERYAUTOM,1000
ENDBLOQUE  
