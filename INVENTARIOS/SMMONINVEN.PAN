CAMPO=NUMINVASIG3,AUXILIAR,CONTEXTUAL=FEJECUTAFORM("SMINVENTARIODESG","S","CODUBI IN (SELECT CODUBI FROM VDUBICA WHERE ALTURA>2) AND STATUS BETWEEN 150 AND 200 AND RECONTEO<VD.GETPROP('CONTEOSJEFE')")
